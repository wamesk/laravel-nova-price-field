{
    "name": "wamesk/laravel-nova-price-field",
    "description": "A Laravel Nova price field with moneyphp/money library.",
    "version": "1.1.3",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "moneyphp/money": "^4.6"
    },
    "autoload": {
        "psr-4": {
            "Wame\\LaravelNovaPriceField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wame\\LaravelNovaPriceField\\Providers\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
